fib1 <- as.numeric(readline(prompt = "Enter a number to start the Fibonacci sequence: "))
num1 <- fib1
num2 <- fib1
print(num1)
repeat {
next_num <- num1 + num2
if (!is.na(next_num) && next_num > 500) {
break
}
print(next_num)
num1 <- num2
num2 <- next_num
}
